PRODUCT_ID := ringap
CLASS_ID:=iptime

CPU_NAME:=mt7620
WIFI_2G:=mt7620
WIFI_5G:=mt7612e
DRAM_SIZE:=64m

FLASH_SIZE:=0x400000
KERNEL_SIZE:=0x170000
EEPROM_FILE:=EEPROM_2G.BIN
EEPROM_FILE_5G:=EEPROM_5G.BIN
DRAM_SIZE_POSTFIX:=64m
# UBOOT Source : /home/mt7621/MT762X_SDK_4300/Uboot.mt7620
BOOT_FILENAME:=uboot.bin.mt7620.$(DRAM_SIZE_POSTFIX)
USE_FACTORY_SECTION:=y
LOAD_ADDRESS := 80000000
LANG_PACKS:=kr,en

PRODUCT_TAG:=$(CPU_NAME).$(WIFI_2G).$(WIFI_5G).$(DRAM_SIZE)$(if $(OPTION_TAG),.$(OPTION_TAG))
KERNEL_FILENAME:=vmlinux.$(PRODUCT_TAG)
KCONFIG_FILE_NAME:=config.$(PRODUCT_TAG)
MODULE_DIR:=$(PRODUCT_TAG)

SDK_VERSION:=SDK_4300
KERNEL_PATH:=../../linux

